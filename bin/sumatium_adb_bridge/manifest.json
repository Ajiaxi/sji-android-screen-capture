{
  "manifest_version": 2,
  "name": "Sumatium Virtual ADB(Android Debug Bridge) Device",
  "version": "1.0",
  "minimum_chrome_version": "33.0.1715.0",
  "permissions": ["system.network"],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "sockets": {
    "tcpServer": {
      "listen": ""
    },
    "tcp": {
      "connect": "*:*"
    }
  },
  "externally_connectable": {
    "matches": ["https://www.sumatium.com/*", "*://localhost.local:*/*"]
  }
}
